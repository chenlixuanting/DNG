<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.guet.navigator.web.pojo.User" table="d_user" lazy="false">
        <id name="userId">
            <generator class="uuid"/>
        </id>
        <timestamp name="updateTime" access="field"/>
        <property name="account" type="string" column="account"/>
        <property name="password" type="string" column="password"/>
        <property name="username" type="string" column="user_name"/>
        <property name="birthday" type="date" column="birthday"/>
        <property name="cdKey" type="string" column="cd_key"/>
        <property name="sex" type="string" column="sex"/>
        <property name="mobile" type="string" column="mobile"/>
        <property name="idCardNumber" type="string" column="id_card_number"/>
        <property name="plateNumber" type="string" column="plate_number"/>
        <property name="headPic" type="string" column="head_pic"/>
        <property name="idCardFrontPic" type="string" column="id_card_front_pic"/>
        <property name="idCardReversePic" type="string" column="id_card_reverse_pic"/>
        <property name="driverLicensePic" type="string" column="driver_license_pic"/>
        <property name="driverPermitPic" type="string" column="driver_permit_pic"/>
        <property name="createTime" type="timestamp">
            <column name="create_time" sql-type="datetime"/>
        </property>
    </class>
</hibernate-mapping>