<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.guet.navigator.web.pojo.PositionData" table="d_position_data">

        <id name="positionDataId" column="pd_id" >
            <generator class="uuid"/>
        </id>

        <property name="latitude" column="latitude" type="double"/>
        <property name="longitude" column="longitude" type="double"/>
        <property name="createTime" column="create_time" type="timestamp"/>

        <many-to-one name="device" column="deviceId" />

    </class>

</hibernate-mapping>